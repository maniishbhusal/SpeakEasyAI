{
    "manifest_version": 3,
    "name": "SpeakEasyAI",
    "version": "1.0.0",
    "description": "Get real time transcription",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [],
    "host_permissions": ["<all_urls>"],
    "web_accessible_resources": [
      {
        "resources": [
          "permission/index.html",
          "permission/requestPermission.js"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }
  